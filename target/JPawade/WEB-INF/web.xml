	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<error-page>
		<error-code>404</error-code>
		<location>/index.jsp</location>
	</error-page>

    <jsp-config>
        <taglib>
            <taglib-uri>JPT</taglib-uri>
            <taglib-location>/WEB-INF/tags.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>JPTcms</taglib-uri>
            <taglib-location>/WEB-INF/cmsTags.tld</taglib-location>
        </taglib>
    </jsp-config>
</web-app>